	<form method="post" class="w-50 mx-auto">

		<div class="form-floating mb-3">
			<input type="text" class="form-control" id="inp1" placeholder="Název">
			<label for="inp1">Název</label>
		</div>
		<div class="form-floating mb-3">
			<input type="text" class="form-control" id="inp2" placeholder="Text">
